{
    "targets": [
        {
            "name": "console",
            "type": "NuLog.Targets.ConsoleTarget",
            "layout": "${Tags} | ${Message}\r\n"
        }
    ],
    "rules": [
        {
            "include": [ "fruit", "info" ],
            "writeTo": [ "console" ]
        }
    ],
    "tagGroups": {
        "fruit": [ "orange", "peach", "tomato" ],
        "debug": [ "info", "fatal" ],
        "info": [ "fatal" ]
    }
}