    {
        "targets": [
            {
                "name": "console",
                "type": "NuLog.Targets.ConsoleTarget",
                "layout": "JSON File: ${Message}\r\n",
                "colorRules": [
                    {
                        "tags": [ "error" ],
                        "foreground": "White",
                        "background": "DarkRed"
                    },
                    {
                        "tags": [ "warn" ],
                        "foreground": "White",
                        "background": "DarkYellow"
                    }
                ],
                "synchronous": false
            }
        ],
        "rules": [
            {
                "include": [ "*" ],
                "writeTo": [ "console" ]
            }
        ]
    }