    {
        "targets": [
            {
                "name": "console",
                "type": "NuLog.Targets.ConsoleTarget"
            },
            {
                "name": "trace",
                "type": "NuLog.Targets.TraceTarget"
            }
        ],
        "rules": [
            {
                "include": [ "consoleonly" ],
                "writeTo": [ "console" ],
                "final": true
            },
            {
                "include": [ "NuLog.Samples.Samples.S1_2_TagsRules.*" ],
                "exclude": [ "notrace" ],
                "writeTo": [ "trace" ],
                "final": false
            },
            {
                "include": [ "mytag" ],
                "writeTo": [ "console" ],
                "final": false
            }
        ]
    }