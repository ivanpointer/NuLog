    {
        "targets": [
            {
                "name": "console",
                "type": "NuLog.Targets.SimpleConsoleTarget",
                "layout": "Machine: ${MachineName} | CPU: ${CPUUsage} | RAM: ${RAMUsage} | ${Message}\r\n"
            }
        ],
        "rules": [
            {
                "include": [ "*" ],
                "writeTo": [ "console" ]
            }
        ],

        "staticMetaDataProviders": [ "NuLog.Samples.CustomizeSamples.S4_3_StaticMetaDataProviders.StaticMetaDataProvider, NuLog.Samples" ]
    }