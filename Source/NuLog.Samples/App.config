<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-4.5.0.0" newVersion="4.5.0.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>

	<appSettings>
		<!-- Settings for our custom configuration builder -->
		<add key="S7_1_ConsoleForeground" value="White" />
		<add key="S7_1_ConsoleBackground" value="DarkGreen" />

		<!-- ... -->

		<add key="S7_1_ConsoleForeground_MEF" value="White" />
		<add key="S7_1_ConsoleBackground_MEF" value="DarkYellow" />
	</appSettings>
</configuration>