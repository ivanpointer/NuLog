{
	"targets": [{
		"name": "file",
		"type": "NuLog.Targets.TextFileTarget",
		"layout": "${Tags} - ${Message}\r\n",
		"fileName": "test.log",
		"oldFileNamePattern": "log{0:.yyyy.MM.dd.hh.mm.ss}.log",

		"rolloverPolicy": "Size",
		"rolloverTrigger": "10MB",
		"oldFileLimit": 10
	}],
	"rules": [{
		"include": ["file"],
		"writeTo": ["file"],
		"final": true
	}],
	"watch": true
}