{
    "targets": [{
        "name": "console",
        "type": "NuLog.Targets.ConsoleTarget",
        "layout": "${longdate}|${tags}|${message}"
    }],
    "rules": [{
        "include": ["cond1"],
        "writeTo": ["target1"],
    },{
        "include": ["cond2"],
        "writeTo": ["target2"],
        "final": true
    },{
        "include": ["cond*"],
        "writeTo": ["target3"],
    }],
    "tagGroups": {
        "trace": ["debug", "info", "warn", "error", "fatal"],
        "debug": ["info", "warn", "error", "fatal"],
        "info": ["warn", "error", "fatal"],
        "warn": ["error", "fatal"],
        "error": ["fatal"]
    },
    "watch": true
}