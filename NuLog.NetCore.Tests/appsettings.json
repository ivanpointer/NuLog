{
    "NuLog": {
        "Targets": [
            {
                "Name": "stream",
                "Type": "NuLog.Targets.DebugTarget",
                "Layout": "${Message}"
            }
        ],
        "Rules": [
            {
                "Includes": [ "one_tag", "two_tag", "red_tag", "blue_tag" ],
                "Excludes": [ "green_tag" ],
                "Targets": [ "stream" ],
                "StrictInclude": true,
                "Final": true
            }
        ],
        "TagGroups": [
            {
                "BaseTag": "base_tag",
                "Aliases": [ "one_tag", "two_tag", "red_tag", "blue_tag" ]
            }
        ],
        "MetaData": {
            "meta": "data"
        }
    }
}